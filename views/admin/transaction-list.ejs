                            </div>
                            <div class=" card-body ">
                                <div class="  table-responsive">
                                    <table id="table" class=" table dataTable ">
                                        <thead>
                                            <tr>
                                                <th>Ngày giao dịch</th>
                                                <th>ID hàng hóa</th>
                                                <th>Chủ sở hữu</td>
                                                <th>Tên giao dịch</th>
                                                <th>Tên doanh nghiệp</th>
                                                <th>Giao dịch viên</th>
                                                <th></th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% for (var i=0; i < transactions.length; i++) { %>
                                                <tr>
                                                    <td>
                                                        <%= transactions[i].transactionDate %>
                                                    </td>
                                                    <td>
                                                        <%= transactions[i].productID %>
                                                    </td>
                                                    <td>
                                                        <%= transactions[i].ownerName %>
                                                    </td>
                                                    <td>
                                                        <%= transactions[i].transactionName %>
                                                    </td>
                                                    <td>
                                                        <%= transactions[i].businessName %>
                                                    </td>
                                                    <td>
                                                        <%= transactions[i].businessAgent %>
                                                    </td>
                                                    <td><a class=" badge bg-primary "
                                                            href="/admin/transaction-details/<%= transactions[i]._id %>">chi
                                                            tiết</a></td>
                                                </tr>
                                                <% } %>
                                        </tbody>
                                    </table>
                                </div>
                            </div>

